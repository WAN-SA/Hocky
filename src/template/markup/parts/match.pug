.match
	.match__inner.row
		mixin matchTeam(item)
			.match__team
				.match__team-img #[img(src=item[2], alt="")]
				.match__team-text
					.match__team-title=item[0]
					.match__team-desc=item[1]
		mixin matchData(p1, p2, classes)
			.match__data(class=classes)
				.match__data-title= p1
				.match__data-desc= p2

		+matchTeam(dataTeams[1])
		+matchData('18:00', 'Парк Легенд', '--left')
		+matchData('9 сентября', 'Москва', '--right')
		+matchTeam(dataTeams[4])



